<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.10119197Latihan3" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorOnPrimary">@color/colorAccent</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MaterialComponents.DayNight.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MaterialComponents.DayNight.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="NunitoBold" parent="TextAppearance.AppCompat">
        <item name="fontFamily">font/Nunito-Bold</item>
    </style>

    <style name="RubikRegular" parent="TextAppearance.AppCompat">
        <item name="fontFamily">font/Rubik-Regular</item>
    </style>

    <style name="TextGunmetalNunitoBold24" parent="NunitoBold">
        <item name="android:textColor">@color/colorGunmetal</item>
        <item name="android:textSize">@dimen/font24</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextNunitoBold24Black87" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/nunito_bold</item>
        <item name="android:textColor">@color/colorBlack87</item>
        <item name="android:textSize">@dimen/font24</item>
    </style>

    <style name="TextBlack87NunitoBold18" parent="NunitoBold">
